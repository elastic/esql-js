{
  "name": "@elastic/esql",
  "version": "0.0.1",
  "author": "Kibana ES|QL team",
  "description": "A set of tools to parse, build and transform ES|QL queries programatically.",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "semantic-release": "semantic-release --dry-run --debug"
  },
  "repository": "https://github.com/elastic/esql-js.git",
  "private": true,
  "license": "To be defined",
  "sideEffects": false,
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/github": "12.0.6",
    "@semantic-release/npm": "13.1.4",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/jest": "29.5.14",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "jest": "29.7.0",
    "prettier": "3.8.1",
    "semantic-release": "25.0.3",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0"
  },
  "dependencies": {
    "antlr4": "4.13.2",
    "tree-dump": "1.1.0",
    "tslib": "2.8.1"
  }
}
