{
  "name": "@elastic/esql",
  "version": "0.0.1",
  "author": "Kibana ES|QL team",
  "description": "A set of tools to parse, build and transform ES|QL queries programatically.",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "repository": "https://github.com/elastic/esql-js.git",
  "private": true,
  "license": "To be defined",
  "sideEffects": false,
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@types/jest": "30.0.0",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "jest": "30.2.0",
    "prettier": "3.8.1",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
